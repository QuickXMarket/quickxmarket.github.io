{
  "appId": "io.ionic.quickxmarket",
  "appName": "QuickXMarket",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "dist",
  "cordova": {},
  "android": {
    "scheme": "quickxmarket"
  },

  "plugins": {
    "CapacitorHttp": {
      "enabled": true
    },
    "StatusBar": {
      "overlaysWebView": false,
      "visible": true
    },
    "SplashScreen": {
      "launchAutoHide": false,
      "showSpinner": false,
      "androidSplashResourceName": "splash",
      "androidScaleType": "CENTER_CROP"
    }
  }
}
